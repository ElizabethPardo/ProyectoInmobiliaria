@model Inmobiliaria.Models.Contrato

@{ ViewData["Title"] = "Eliminar"; }

<h2>Eliminar Contrato</h2>

<h3>¿Seguro que desea eliminar el contrato?</h3>
<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Id)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Id)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.FechaDesde)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FechaDesde)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.FechaHasta)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FechaHasta)
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Inquilino)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Inquilino.Nombre)
            @Html.DisplayFor(model => model.Inquilino.Apellido)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Inmueble)
        </dt>
        <dd class="col-sm-10">
           
            @Html.DisplayFor(model => model.Inmueble.Direccion)
           
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="submit" value="Eliminar" class="btn btn-danger" /> 
        <a  asp-action="Index" class="btn btn-secondary">Volver</a>
    </form>
</div>
